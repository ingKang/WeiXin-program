.imgbox{
  position: absolute;
  animation: imgsport 15s infinite;
}
.img{
  position: absolute;
  top: 2rpx;
  width: 750rpx;
}
 @keyframes imgsport{
    0%{
      left:0;
    }
    25%{
      left:-750rpx
    }
    50%{
      left:-1500rpx
    }
    75%{
      left:-2250rpx
    }
    100%{
      left:-3000rpx
    }
  }
.img1{
  left: 0
}
.img2{
  left: 750rpx
}
.img3{
  left: 1500rpx
}
.img4{
  left: 2250rpx
}
.img5{
  left: 3000rpx
}